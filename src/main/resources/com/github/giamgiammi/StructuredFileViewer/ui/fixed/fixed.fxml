<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane fx:id="rootPane" prefHeight="517.0" prefWidth="661.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.giamgiammi.StructuredFileViewer.ui.fixed.FixedWidthSettingsController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="537.0" minWidth="10.0" prefWidth="525.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="211.0" minWidth="10.0" prefWidth="133.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints maxHeight="531.0" minHeight="10.0" prefHeight="454.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="133.0" minHeight="0.0" prefHeight="45.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="137.0" minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn prefWidth="138.0" text="%fixed.settings.column.name" />
          <TableColumn prefWidth="97.0" text="%fixed.settings.column.length" />
            <TableColumn prefWidth="149.0" text="%fixed.settings.column.trim" />
        </columns>
      </TableView>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1">
         <children>
            <Button layoutX="26.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleAddCol" prefHeight="26.0" prefWidth="78.0" text="%fixed.settings.button.add" />
            <Button layoutX="26.0" layoutY="51.0" mnemonicParsing="false" onAction="#handleEditCol" prefHeight="26.0" prefWidth="78.0" text="%fixed.settings.button.edit" />
            <Button layoutX="26.0" layoutY="88.0" mnemonicParsing="false" onAction="#handleDeleteCol" prefHeight="26.0" prefWidth="78.0" text="%fixed.settings.button.delete" />
         </children>
      </AnchorPane>
      <CheckBox fx:id="recordEndsWithNewLine" mnemonicParsing="false" text="%fixed.settings.record_ends_newline" GridPane.columnSpan="2" GridPane.rowIndex="1" />
      <FlowPane alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="2">
         <children>
            <Label prefHeight="17.0" text="%fixed.settings.charset" />
            <Label text="     " />
            <ComboBox fx:id="charset" editable="true" prefHeight="26.0" prefWidth="209.0" />
         </children>
      </FlowPane>
   </children>
</GridPane>
